{
  "Admin": {
    "BasePath": "admin",
    "Title": "Krakenar API",
    "Version": "1.1.0"
  },
  "AllowedHosts": "*",
  "Authentication": {
    "AccessToken": {
      "Type": "at+jwt",
      "LifetimeSeconds": 300
    }
  },
  "Caching": {
    "ActorLifetime": "00:15:00"
  },
  "Cookies": {
    "RefreshToken": {
      "HttpOnly": true,
      "MaxAge": "7.00:00:00",
      "SameSite": "Strict",
      "Secure": true
    },
    "Session": {
      "SameSite": "Strict"
    }
  },
  "Cors": {
    "AllowAnyOrigin": false,
    "AllowedOrigins": [],
    "AllowAnyMethod": false,
    "AllowedMethods": [ "GET" ],
    "AllowAnyHeader": false,
    "AllowedHeaders": [],
    "AllowCredentials": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MongoDB": {
    "DatabaseName": "cms",
    "EnableLogging": true
  },
  "Passwords": {
    "Pbkdf2": {
      "Algorithm": "HMACSHA256",
      "Iterations": 600000,
      "SaltLength": 32,
      "HashLength": null
    }
  },
  "Retry": {
    "Algorithm": "Exponential",
    "Delay": 100,
    "ExponentialBase": 2,
    "RandomVariation": 0,
    "MaximumRetries": 5,
    "MaximumDelay": 0
  }
}
